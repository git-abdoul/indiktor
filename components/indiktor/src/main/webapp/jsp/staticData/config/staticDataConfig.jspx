					</td>
				</tr>
				<tr>
					<td align="center">
						<ice:selectOneMenu value="#{staticDataConfigBean.logicalEnvSelectionBean.logicalEnvId}"
										   valueChangeListener="#{staticDataConfigBean.logicalEnvSelectionBean.onChangeLogicalEnv}"
										   partialSubmit="true">
