	<ice:panelPopup draggable="false" modal="true" autoCentre="true" rendered="#{dataCenterBean.rendererUpdateStaticData}">
	    <f:facet name="header">
	    	<ice:panelGroup style="height: 25px;">
		       	<table width="100%">
		       		<tr>
		       			<td><ice:outputLabel style="float: left;" value="Update StaticData"/></td>
		       			<td>
		       				<ice:commandButton style="float: right;" 
		       								   actionListener="#{dataCenterBean.closeUpdateStaticDataPopup}"
		       								   image="/images/i/cal_off.gif"
		       								   alt="Close"/>
							<ice:commandButton style="float: right;" 
											   image="/images/IndiKtor2.0/Buttons/saveSmall.png"
										   	   title="Save"
										   	   value="Save"
										   	   styleClass="iconPopup"
										   	   actionListener="#{dataCenterBean.saveStaticData}"/>		       								   
						</td>
		       		</tr>
		       	</table> 
		    </ice:panelGroup>
	    </f:facet>
	    <f:facet name="body">
			<ice:panelGrid id="updateStaticData">
				<ui:include src="/jsp/staticData/config/updateStaticData.jspx"/>
			</ice:panelGrid>
	    </f:facet>
	</ice:panelPopup>
	
	<ice:panelPopup draggable="false" modal="true" autoCentre="true" rendered="#{dataCenterBean.rendererNewStaticData}">
	    <f:facet name="header">
	    	<ice:panelGroup style="height: 25px;">
		       	<table width="100%">
		       		<tr>
		       			<td><ice:outputLabel style="float: left;" value="Create new static data"/></td>
		       			<td>
		       				<ice:commandButton style="float: right;" 
		       								   actionListener="#{dataCenterBean.closeNewStaticDataPopup}"
		       								   image="/images/i/cal_off.gif"
		       								   alt="Close"/>
							<ice:commandButton style="float: right;" 
											   image="/images/IndiKtor2.0/Buttons/saveSmall.png"
										   	   title="Save"
										   	   value="Save"
										   	   styleClass="iconPopup"
										   	   actionListener="#{dataCenterBean.saveStaticData}"/>		       								   
						</td>
		       		</tr>
		       	</table> 
		    </ice:panelGroup>
	    </f:facet>
	    <f:facet name="body">
			<ice:panelGrid id="updateStaticData">
				<ui:include src="/jsp/staticData/config/updateStaticData.jspx"/>
			</ice:panelGrid>
	    </f:facet>
	</ice:panelPopup>
		
</ice:form>
</f:subview>